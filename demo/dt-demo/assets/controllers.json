{
    "controllers": {
        "@survos/api-grid-bundle": {
            "api_grid": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "datatables/media/css/jquery.dataTables.min.css": true,
                    "datatables.net-bs5/css/dataTables.bootstrap5.min.css": true
                }
            }
        },
        "@survos/datatables-bundle": {
            "table": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@survos/grid-bundle": {
            "grid": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "datatables/media/css/jquery.dataTables.min.css": true,
                    "datatables.net-bs5/css/dataTables.bootstrap5.min.css": true
                }
            }
        },
        "@survos/html-prettify-bundle": {
            "prettify": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@survos/simple-datatables-bundle": {
            "table": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "@survos/simple-datatables/style.css": true
                }
            }
        }
    },
    "entrypoints": []
}
