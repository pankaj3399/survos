{
    "type": "project",
    "license": "proprietary",
    "name": "survos/mono",
    "description": "mono test",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^6.1",
        "symfony/http-kernel": "^6.1.0",
        "symfony/runtime": "^6.1",
        "symfony/yaml": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/monorepo-builder": "^11.1"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "YourMonorepo\\FirstPackage\\": "packages/first-package/src",
            "YourMonorepo\\SecondPackage\\": "packages/second-package/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "your-monorepo/first-package": "5.4.2",
        "your-monorepo/second-package": "5.4.2"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.1"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
