{
    "license": "proprietary",
    "name": "survos/survos",
    "description": "survos monorepo -- bundles and demos",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        },
        {
            "name": "Nacho Mart√≠n",
            "email": "nacho@limenius.com"
        }
    ],
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "answear/liform": "^0.20",
        "api-platform/core": "^3.2",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.12",
        "doctrine/persistence": "^2.0 || ^3.0",
        "fabpot/goutte": "^4.0",
        "fakerphp/faker": "^1.20",
        "freearhey/wikidata": "^3.6",
        "friendsofsymfony/jsrouting-bundle": "^3.3",
        "gajus/dindent": "^2.0",
        "haydenpierce/class-finder": "^0.4.3",
        "illuminate/collections": "^10.9",
        "jefs42/libretranslate": "^1.1",
        "knplabs/knp-menu-bundle": "^3.2",
        "knpuniversity/oauth2-client-bundle": "^2.10",
        "league/csv": "^9.9",
        "meilisearch/search-bundle": "^0.11.0",
        "picqer/php-barcode-generator": "^2.2",
        "roave/better-reflection": "^6.0",
        "sabre/xml": "^4.0",
        "sebastian/diff": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "survos/bootstrap-bundle": "^1.4",
        "survos/command-bundle": "^1.4",
        "survos/grid-bundle": "^1.4",
        "symfony/cache": "^6.1",
        "symfony/config": "^6.3 || ^7.0",
        "symfony/console": "^6.3 || ^7.0",
        "symfony/dependency-injection": "^6.3 || ^7.0",
        "symfony/dotenv": "^6.3 || ^7.0",
        "symfony/finder": "^6.3 || ^7.0",
        "symfony/flex": "^2.0",
        "symfony/form": "^6.3 || ^7.0",
        "symfony/framework-bundle": "^6.3 || ^7.0",
        "symfony/http-client": "^6.3 || ^7.0",
        "symfony/http-kernel": "^6.3 || ^7.0",
        "symfony/messenger": "^6.3 || ^7.0",
        "symfony/options-resolver": "^6.3 || ^7.0",
        "symfony/password-hasher": "^6.3 || ^7.0",
        "symfony/process": "^6.3 || ^7.0",
        "symfony/runtime": "^6.3 || ^7.0",
        "symfony/security-bundle": "^6.3 || ^7.0",
        "symfony/serializer": "^6.3 || ^7.0",
        "symfony/stopwatch": "^6.1",
        "symfony/string": "^6.3 || ^7.0",
        "symfony/ux-twig-component": "^2.8",
        "symfony/validator": "^6.3 || ^7.0",
        "symfony/workflow": "^6.3 || ^7.0",
        "symfony/yaml": "^6.3 || ^7.0",
        "tacman/twig-tree-tag": "^3.0.1",
        "twig/extra-bundle": "^3.4",
        "twig/string-extra": "^3.4",
        "twig/twig": "^3.4",
        "zenstruck/console-extra": "^1.1"
    },
    "require-dev": {
        "behat/behat": "^3.13",
        "ekino/phpstan-banned-code": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpdocumentor/reflection-docblock": "*",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15",
        "roave/security-advisories": "dev-latest",
        "spaze/phpstan-disallowed-calls": "^2.14",
        "squizlabs/php_codesniffer": "^3.7",
        "survos/maker-bundle": "*@dev",
        "symfony/browser-kit": "^6.3 || ^7.0",
        "symfony/debug-bundle": "^6.3 || ^7.0",
        "symfony/maker-bundle": "^1.38",
        "symfony/phpunit-bridge": "^6.3 || ^7.0",
        "symfony/polyfill-php80": "^1.16.0",
        "symfony/security-core": "^6.3 || ^7.0",
        "symfony/security-csrf": "^6.3 || ^7.0",
        "symfony/twig-bundle": "^6.3 || ^7.0",
        "symfony/var-dumper": "^6.3 || ^7.0",
        "symfony/web-profiler-bundle": "^6.3 || ^7.0",
        "symplify/config-transformer": "^11.1",
        "symplify/easy-coding-standard": "^11.1",
        "symplify/monorepo-builder": "11.2.2.72",
        "tacman/composer-json-manipulator": "^11.1"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "endroid/installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "exclude-from-classmap": [
            "packages/location-bundle/Tests/"
        ],
        "psr-4": {
            "App\\": "src/",
            "Limenius\\LiformBundle\\": "packages/liform-bundle/",
            "Survos\\ApiGrid\\": "packages/api-grid-bundle/src/",
            "Survos\\AuthBundle\\": "packages/auth-bundle/src/",
            "Survos\\BarcodeBundle\\": "packages/barcode-bundle/src/",
            "Survos\\BootstrapBundle\\": "packages/bootstrap-bundle/src/",
            "Survos\\BootstrapBundle\\Tests\\": "packages/bootstrap-bundle/tests/",
            "Survos\\Bundle\\MakerBundle\\": "packages/maker-bundle/src/",
            "Survos\\CommandBundle\\": "packages/command-bundle/src/",
            "Survos\\CommandBundle\\Tests\\": "packages/command-bundle/tests/",
            "Survos\\CoreBundle\\": "packages/core-bundle/src/",
            "Survos\\CrawlerBundle\\": "packages/crawler-bundle/src/",
            "Survos\\DocBundle\\": "packages/doc-bundle/src/",
            "Survos\\FakerBundle\\": "packages/faker-bundle/src/",
            "Survos\\FooBundle\\": [
                "packages/import-bridge/src/",
                "packages/ruler-bundle/src/",
                "packages/stripe-product-bundle/src/"
            ],
            "Survos\\GridGroupBundle\\": "packages/grid-group-bundle/src/",
            "Survos\\GridGroupBundle\\Tests\\": [
                "packages/doc-bundle/tests/",
                "packages/grid-group-bundle/tests/"
            ],
            "Survos\\GridGroup\\": "packages/grid-group/src/",
            "Survos\\Grid\\": "packages/grid-bundle/src/",
            "Survos\\HtmlPrettifyBundle\\": "packages/html-prettify-bundle/src/",
            "Survos\\ImportBridge\\": "packages/import-bridge/src/",
            "Survos\\InspectionBundle\\": "packages/inspection-bundle/src/",
            "Survos\\LocationBundle\\": "packages/location-bundle/src/",
            "Survos\\MeiliAdminBundle\\": "packages/meili-admin-bundle/src/",
            "Survos\\Providence\\": "packages/providence-bundle/src/",
            "Survos\\RulerBundle\\": "packages/ruler-bundle/src/",
            "Survos\\Scraper\\": "packages/scraper-bundle/src/",
            "Survos\\Scraper\\Tests\\": "packages/scraper-bundle/tests/",
            "Survos\\StripeProduct\\": "packages/stripe-product/src/",
            "Survos\\Tree\\": "packages/tree-bundle/src/",
            "Survos\\WikiBundle\\": "packages/wiki-bundle/src/",
            "Survos\\WorkflowBundle\\": "packages/workflow-helper-bundle/src/",
            "Survos\\RevealBundle\\": "packages/reveal-bundle/src/",
            "Survos\\LibreTranslateBundle\\": "packages/libre-translate-bundle/src/",
            "Survos\\DeploymentBundle\\": "packages/deployment-bundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Survos\\LocationBundle\\Tests\\": "packages/location-bundle/tests/"
        }
    },
    "extra": {
        "symfony": {
            "require": "^6.1",
            "allow-contrib": "true"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit packages --coverage-html coverage\n"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "repositories": {
        "velzon": {
            "type": "vcs",
            "url": "git@github.com:survos/velzon-bundle.git"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
